<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sijiagao.whatsfordinner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Whats for Dinner"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RecipeActivity" />
        <activity android:name=".activity.MealActivity" />
        <activity android:name=".activity.GroceryActivity" />
        <activity android:name=".activity.NewdishActivity" />
        <activity android:name=".activity.DayMealActivity" />
        <activity android:name=".activity.UpDateRecipeActivity" />
        <activity android:name=".activity.ListDemoActivity"></activity>
    </application>

</manifest>